{
  "name": "smartercleanup-django-client",
  "version": "3.19.1",
  "description": "Standalone Shareabouts client and server package",
  "engines": {
    "node": ">=8.0"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/smartercleanup/platform.git"
  },
  "keywords": [
    "maps",
    "crowdsourcing",
    "engagement"
  ],
  "author": "SmarterCleanup",
  "license": "GPL",
  "bugs": {
    "url": "https://github.com/smartercleanup/platform/issues"
  },
  "homepage": "https://github.com/smartercleanup/platform",
  "scripts": {
    "build-scss": "./scripts/run-node-sass.sh",
    "watch-scss": "./scripts/run-node-sass.sh -w",
    "build-scss-production": "./scripts/run-node-sass.sh -p",
    "start": "node static-build.js && npm run build-scss && npm run watch-scss & npm run watch-js & webpack-dev-server",
    "build-js": "webpack -d",
    "build-js-production": "NODE_ENV=production webpack -p && cat www/bundle.js | gzip > www/bundle.js.gz",
    "watch-js": "webpack -d --watch",
    "build-css": "cat src/base/static/dist/cat-bundle.css | tee www/dist/bundle.css | gzip > www/dist/bundle.css.gz",
    "build": "NODE_ENV=production node static-build.js && npm run build-scss-production && npm run build-js-production && npm run build-css",
    "prettier": "prettier --write --trailing-comma all \"{src,blah}/**/*.{js,jsx}\""
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "babel-runtime": "^6.23.0",
    "compass-importer": "^0.4.1",
    "dotenv": "^4.0.0",
    "esprima": "^3.1.3",
    "glob": "^7.1.1",
    "accessible-autocomplete": "git+https://github.com/mapseed/accessible-autocomplete.git",
    "node-env-file": "^0.1.7",
    "node-sass": "^4.0.0",
    "npm": "^5.2.0",
    "shelljs": "^0.5.3",
    "static-eval": "^1.1.1",
    "togeojson": "^0.16.0",
    "webpack": "~2.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-es2015": "^6.24.1",
    "catw": "^1.0.1",
    "eslint-config-prettier": "^2.1.1",
    "gettext-parser": "^1.2.2",
    "handlebars": "^4.0.10",
    "js-yaml": "^3.9.0",
    "mv": "^2.1.1",
    "node-gettext": "^2.0.0",
    "object-walk": "^0.1.1",
    "prettier": "^1.4.4",
    "recursive-copy": "^2.0.6",
    "watchify": "^3.4.0",
    "wax-on": "^1.2.0",
    "webpack-dev-server": "^2.6.1"
  }
}
