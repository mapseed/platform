          {
            "id": "sample-raster-tile-basemap",
            "type": "raster",
            "source": "sample-raster-tile"
          }
        ]
      },
      {
        "id": "sample-json-b",
        "popupContent": "<h3>City name:</h3><p>{{CITYNAME}}</p>",
        "mapboxLayers": [
          {
            "id": "sample-json-b-labels",
            "type": "symbol",
            "source": "sample-json-b",
            "paint": {
              "text-halo-color": "#fff",
              "text-halo-width": 1
            },
            "layout": {
              "text-field": ["get", "CITYNAME"],
              "text-size": 14,
              "text-allow-overlap": false,
              "text-offset": [0, 2]
            }
          },
          {
            "id": "sample-json-b-fill",
            "type": "fill",
            "source": "sample-json-b",
            "paint": {
              "fill-opacity": 0.6,
              "fill-outline-color": "#000000",
              "fill-color": [
                "match",
                ["to-number", ["get", "LID_Checklist_Master_score"]],
                5,
                "#fef0d9",
                4,
                "#fdcc8a",
                3,
                "#fc8d59",
                2,
                "#e34a33",
                1,
                "#b30000",
                "#000"
              ]
            }
          }
        ]
      },
      {
        "id": "sample-vector-a",
        "mapboxLayers": [
          {
            "id": "sample-vector-a",
            "type": "fill",
            "source": "sample-vector-a",
            "source-layer": "historical_river_pathgeojson",
            "paint": {
              "fill-color": [
                "match",
                ["get", "landcover"],
                "channel",
                "#3366ff",
                "tideflat",
                "#000099",
                "barrier beach",
                "#ffcc00",
                "barrier beach spitt/grass",
                "#00cc00",
                "barrier beach/grass",
                "#339933",
                "estuarine emergent marsh",
                "#99cc00",
                "pond",
                "#66ccff",
                "salt pond",
                "#9999ff",
                "#000"
              ]
            }
          }
        ]
      },
      {
        "id": "sample-vector-b",
        "mapboxLayers": [
          {
            "id": "sample-vector-b-fill",
            "type": "fill",
            "source": "sample-vector-b",
            "source-layer": "palouse-critical-aquifer",
            "paint": {
              "fill-color": "#0066ff",
              "fill-opacity": 0.6
            }
          },
          {
            "id": "sample-vector-b-outline",
            "type": "line",
            "source": "sample-vector-b",
            "source-layer": "palouse-critical-aquifer",
            "paint": {
              "line-color": "#0066ff",
              "line-opacity": 0.8
            }
          }
        ]
      },
      {
        "id": "sample-json-a",
        "visibleDefault": true,
        "mapboxLayers": [
          {
            "id": "sample-json-a",
            "type": "symbol",
            "source": "sample-json-a",
            "layout": {
              "icon-image": "marker-conserve-water.png",
              "icon-size": 0.5,
              "icon-anchor": "center",
              "icon-allow-overlap": true
            }
          }
        ]
      },
      {
        "id": "demo",
        "datasetSlug": "demo",
        "interactive": true,
        "visibleDefault": true,
        "mapboxFocusedLayers": [
          {
            "id": "demo-focused",
            "source": "demo",
            "type": "symbol",
            "layout": {
              "icon-size": 1.2,
              "icon-anchor": "bottom",
              "icon-image": [
                "match",
                ["get", "location_type"],
                "complaint",
                "marker-complaint.png",
                "question",
                "marker-question.png",
                "observation",
                "marker-observation.png",
                "idea",
                "marker-idea.png",
                "__no-icon-image__"
              ]
            }
          }
        ],
        "mapboxLayers": [
          {
            "id": "demo",
            "source": "demo",
            "type": "symbol",
            "layout": {
              "icon-allow-overlap": true,
              "icon-size": ["step", ["zoom"], 0.5, 11, 0.75, 14, 0.9],
              "icon-anchor": "bottom",
              "icon-image": [
                "match",
                ["get", "location_type"],
                "complaint",
                "marker-complaint.png",
                "observation",
                "marker-observation.png",
                "idea",
                "marker-idea.png",
                "question",
                "marker-question.png",
                "__no-icon-image__"
              ]
            }
          }
        ]
      },
      {
        "id": "demo-featured",
        "datasetSlug": "demo-featured",
        "interactive": true,
        "visibleDefault": true,
        "mapboxFocusedLayers": [
          {
            "id": "demo-featured-focused-points",
            "type": "symbol",
            "source": "demo-featured",
            "filter": ["==", ["get", "location_type"], "demo-featured"],
            "layout": {
              "icon-allow-overlap": true,
              "icon-size": 1.2,
              "icon-anchor": "bottom"
            }
          }
        ],
        "mapboxLayers": [
          {
            "id": "demo-featured-points",
            "type": "symbol",
            "source": "demo-featured",
            "filter": ["==", ["geometry-type"], "Point"],
            "layout": {
              "icon-allow-overlap": true,
              "icon-size": 0.5,
              "icon-anchor": "bottom",
              "icon-image": [
                "case",
                ["has", "marker-symbol", ["get", "style"]],
                ["get", "marker-symbol", ["get", "style"]],
                "map-pin-star-marker.png"
              ]
            }
          },
          {
            "id": "demo-featured-lines",
            "type": "line",
            "source": "demo-featured",
            "filter": ["==", ["geometry-type"], "LineString"],
            "paint": {
              "line-color": [
                "case",
                ["has", "stroke", ["get", "style"]],
                ["get", "stroke", ["get", "style"]],
                "#f86767"
              ],
              "line-opacity": [
                "case",
                ["has", "stroke-opacity", ["get", "style"]],
                ["get", "stroke-opacity", ["get", "style"]],
                0.7
              ]
            }
          },
          {
            "id": "demo-featured-polygons",
            "type": "fill",
            "source": "demo-featured",
            "filter": ["==", ["geometry-type"], "Polygon"],
            "paint": {
              "fill-color": [
                "case",
                ["has", "fill", ["get", "style"]],
                ["get", "fill", ["get", "style"]],
                "#f86767"
              ],
              "fill-opacity": [
                "case",
                ["has", "fill-opacity", ["get", "style"]],
                ["get", "fill-opacity", ["get", "style"]],
                0.7
              ]
            }
          },
          {
            "id": "demo-featured-polygon-outlines",
            "type": "line",
            "source": "demo-featured",
            "filter": ["==", ["geometry-type"], "Polygon"],
            "paint": {
              "line-color": [
                "case",
                ["has", "stroke", ["get", "style"]],
                ["get", "stroke", ["get", "style"]],
                "#f86767"
              ],
              "line-opacity": [
                "case",
                ["has", "stroke-opacity", ["get", "style"]],
                ["get", "stroke-opacity", ["get", "style"]],
                0.7
              ]
            }
          }
        ]
      }
    ]
