          {
            "symbol-layout": {
              "icon-size": 0.7,
              "icon-anchor": "bottom",
              "icon-image": [
                "match",
                ["get", "location_type"],
                "reports",
                "map-pin-marker-red.png",
                "__no-icon-image__"
              ]
